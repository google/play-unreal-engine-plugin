<root xmlns:android="http://schemas.android.com/apk/res/android">
    <resourceCopies>
        <log text="Copying proguard files for Play Core SDK"/>
        <copyDir src="$S(AbsPluginDir)/../ThirdParty/play-core-native-sdk/proguard"
                 dst="$S(BuildDir)/gradle/app/proguard" />

        <log text="Copying tracking metadata files for Unreal Engine plugins"/>
        <copyDir src="$S(AbsPluginDir)/../ThirdParty/play-core-native-sdk/tracking"
                 dst="$S(BuildDir)/gradle/app/tracking" />
        <copyDir src="$S(AbsPluginDir)/../ThirdParty/play-core-native-sdk/tracking"
                 dst="$S(BuildDir)/gradle/AFSProject/app/tracking" />
    </resourceCopies>
</root>
